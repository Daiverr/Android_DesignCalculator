<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginBottom="54dp"
        android:elevation="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/button24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/systemin" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="0dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button3"
        app:srcCompat="@drawable/square" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:fontFamily="@font/sfcompactdisplay"
        android:gravity="center"
        android:text="="
        android:textColor="#FFFFFF"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button3"
        tools:text="=" />

    <TextView
        android:id="@+id/inner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="8dp"
        android:elevation="4dp"
        android:fontFamily="@font/sfcompactdisplay"
        android:gravity="center"
        android:text="0"
        android:textColor="@android:color/black"
        android:textSize="42sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView10"
        app:layout_constraintEnd_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10"
        tools:text="0" />

    <Button
        android:id="@+id/button"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:text="00"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="00" />

    <Button
        android:id="@+id/button2"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:text="0"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintStart_toEndOf="@+id/button"
        android:onClick="number"
        tools:text="0" />

    <Button
        android:id="@+id/button3"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:text="."
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton2"
        app:layout_constraintStart_toEndOf="@+id/button2"
        tools:text="." />

    <Button
        android:id="@+id/button5"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:onClick="number"
        android:text="1"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toStartOf="@+id/button6"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="1" />

    <Button
        android:id="@+id/button6"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:onClick="number"
        android:text="2"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button2"
        app:layout_constraintEnd_toStartOf="@+id/button7"
        app:layout_constraintStart_toEndOf="@+id/button5"
        tools:text="2" />

    <Button
        android:id="@+id/button7"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:onClick="number"
        android:text="3"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toStartOf="@+id/button8"
        app:layout_constraintStart_toEndOf="@+id/button6"
        tools:text="3" />

    <Button
        android:id="@+id/button8"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:text="+"
        android:textColor="#4C91E4"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button7"
        tools:text="+" />

    <Button
        android:id="@+id/button9"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:onClick="number"
        android:text="4"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toStartOf="@+id/button11"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="4" />

    <Button
        android:id="@+id/button11"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:onClick="number"
        android:text="5"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintEnd_toStartOf="@+id/button12"
        app:layout_constraintStart_toEndOf="@+id/button9"
        tools:text="5" />

    <Button
        android:id="@+id/button12"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:onClick="number"
        android:text="6"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toStartOf="@+id/button13"
        app:layout_constraintStart_toEndOf="@+id/button11"
        tools:text="6" />

    <Button
        android:id="@+id/button13"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:text="-"
        android:textColor="#4C91E4"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button12"
        tools:text="-" />

    <Button
        android:id="@+id/button18"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:onClick="number"
        android:text="7"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toStartOf="@+id/button11"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="7" />

    <Button
        android:id="@+id/button19"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:onClick="number"
        android:text="8"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintEnd_toStartOf="@+id/button12"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/button9"
        tools:text="8" />

    <Button
        android:id="@+id/button20"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:onClick="number"
        android:text="9"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintEnd_toStartOf="@+id/button13"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/button11"
        tools:text="9" />

    <Button
        android:id="@+id/button21"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:text="×"
        android:textColor="#4C91E4"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button20"
        tools:text="×" />

    <Button
        android:id="@+id/button22"
        android:layout_width="0dp"
        android:layout_height="103.5dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:paddingTop="20dp"
        android:text="C"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button18"
        app:layout_constraintEnd_toStartOf="@+id/button23"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="clear"
        tools:text="C" />

    <Button
        android:id="@+id/button23"
        android:layout_width="0dp"
        android:layout_height="103.5dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:paddingTop="20dp"
        android:text="0"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button18"
        app:layout_constraintEnd_toStartOf="@+id/button24"
        app:layout_constraintStart_toEndOf="@+id/button22"
        tools:text="0" />

    <Button
        android:id="@+id/button24"
        android:layout_width="0dp"
        android:layout_height="103.5dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:paddingTop="20dp"
        android:text="%"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button19"
        app:layout_constraintEnd_toStartOf="@+id/button25"
        app:layout_constraintStart_toEndOf="@+id/button23"
        tools:text="%" />

    <Button
        android:id="@+id/button25"
        android:layout_width="0dp"
        android:layout_height="103.5dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sfcompactdisplay"
        android:paddingTop="20dp"
        android:text="÷"
        android:textColor="#4C91E4"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/button20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button24"
        tools:text="÷" />

</android.support.constraint.ConstraintLayout>