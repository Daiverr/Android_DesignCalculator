<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="90dp"
        android:layout_height="74dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="center"
        android:text="7"
        android:elevation="2dp"
        android:textColor="#3E3E3E"
        android:textSize="43sp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="90dp"
        android:layout_height="74dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="center"
        android:elevation="2dp"
        android:text="C"
        android:textColor="#3E3E3E"
        android:textSize="43sp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="C" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="90dp"
        android:layout_height="74dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="center"
        android:elevation="2dp"
        android:text="4"
        android:textColor="#3E3E3E"
        android:textSize="43sp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="90dp"
        android:layout_height="74dp"
        android:elevation="2dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="center"
        android:text="1"
        android:textColor="#3E3E3E"
        android:textSize="43sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="90dp"
        android:layout_height="74dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="center"
        android:elevation="2dp"
        android:text="00"
        android:textColor="#3E3E3E"
        android:textSize="43sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="00" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="90dp"
        android:layout_height="74dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="center"
        android:elevation="2dp"
        android:text="0"
        android:textColor="#3E3E3E"
        android:textSize="43sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        tools:text="0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="90dp"
        android:layout_height="74dp"
        android:elevation="2dp"
        android:gravity="center"
        android:text="."
        android:textColor="#3E3E3E"
        android:textSize="43sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton2"
        tools:text="." />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="90dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/whitesquare" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginTop="118dp"
        android:elevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:srcCompat="@drawable/systemin" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="168dp"
        android:scaleType="center"
        app:layout_constraintDimensionRatio="H,360:168"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rec_gray" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="90dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/square" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="90dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/imageButton2"
        app:srcCompat="@drawable/whitesquare" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="90dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:srcCompat="@drawable/whitesquare"
        tools:layout_editor_absoluteX="180dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="90dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton7"
        app:srcCompat="@drawable/whitesquare"
        tools:layout_editor_absoluteX="180dp" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="90dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton8"
        app:srcCompat="@drawable/whitesquare"
        tools:layout_editor_absoluteX="180dp" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="90dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton9"
        app:srcCompat="@drawable/whitesquare"
        tools:layout_editor_absoluteX="180dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="90dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:srcCompat="@drawable/whitesquare" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="90dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:srcCompat="@drawable/whitesquare" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="90dp"
        android:layout_height="74dp"
        android:gravity="center"
        android:text="="
        android:textColor="#FFFFFF"
        android:textSize="43sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="=" />

</android.support.constraint.ConstraintLayout>